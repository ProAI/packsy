{
  "name": "packsy",
  "version": "0.2.2",
  "description": "Start development of a package with pre-defined eslint, prettier, babel and rollup configuration",
  "author": "Markus Wetzel <markuswetzel@gmx.net>",
  "license": "MIT",
  "keywords": [
    "package development",
    "eslint",
    "prettier",
    "babel",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ProAI/packsy.git"
  },
  "bugs": {
    "url": "https://github.com/ProAI/packsy/issues"
  },
  "bin": {
    "eslint": "./src/scripts/lint.js",
    "packsy": "./src/scripts/packsy.js"
  },
  "scripts": {
    "prepublishOnly": "node ./src/scripts/validate.js"
  },
  "files": [
    "src",
    "config-eslint.js",
    "config-prettier.js"
  ],
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "lodash.camelcase": "^4.3.0",
    "micromatch": "^4.0.2",
    "mkdirp": "^1.0.4",
    "npm-which": "^3.0.1",
    "prettier": "2.2.1",
    "read-pkg-up": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "yargs-parser": "^20.2.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./src/scripts/pre-commit.js"
    }
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
