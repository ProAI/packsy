{
  "name": "packsy",
  "version": "0.2.1",
  "description": "Start development of a package with pre-defined eslint, prettier, babel and rollup configuration",
  "author": "Markus Wetzel <markuswetzel@gmx.net>",
  "license": "MIT",
  "keywords": [
    "package development",
    "eslint",
    "prettier",
    "babel",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ProAI/packsy.git"
  },
  "bugs": {
    "url": "https://github.com/ProAI/packsy/issues"
  },
  "bin": {
    "eslint": "./src/scripts/lint.js",
    "packsy": "./src/scripts/packsy.js"
  },
  "scripts": {
    "prepublishOnly": "node ./src/scripts/validate.js"
  },
  "files": [
    "src",
    "config-eslint.js",
    "config-prettier.js"
  ],
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "lodash.camelcase": "^4.3.0",
    "micromatch": "^4.0.2",
    "mkdirp": "^1.0.3",
    "npm-which": "^3.0.1",
    "prettier": "1.19.1",
    "read-pkg-up": "^7.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.3.0",
    "yargs-parser": "^18.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./src/scripts/pre-commit.js"
    }
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
